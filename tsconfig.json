{
	"include": ["src", "test"],
	"compilerOptions": {
		"incremental": true,
		"target": "esnext",
		"module": "commonjs",
		"lib": ["esnext"],
		"outDir": "./dist",
		"downlevelIteration": true,
		"strict": true,
		"noImplicitAny": true,
		"allowUnusedLabels": false,
		"allowUnreachableCode": false,
		"noUnusedLocals": true,
		"pretty": true
	}
}
